[
    {

      "portMappings": [
        {
          "hostPort": 8000,
          "protocol": "tcp",
          "containerPort": 8000
        }

      ],
      "cpu": 256,
      "memory": 512,
      "image": "868981377508.dkr.ecr.eu-west-1.amazonaws.com/challenges:latest",
      "healthCheck": {
        "retries": 3,
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost:8000/actuator/health || exit 1"
        ],
        "timeout": 5,
        "interval": 30,
        "startPeriod": 60
      },
      "essential": true,
      "name": "userApi"
    }
]
