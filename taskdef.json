{
    "family": "challenges",
    "containerDefinitions": [
        {
            "image": "%REPOSITORY_URI%:v_%BUILD_NUMBER%",
            "name": "challenges",
	    "executionRoleArn": "arn:aws:iam::868981377508:role/ecsTaskExecutionRole",
            "cpu": "256",
            "memory": "512",
            "essential": "true",
	    "family": "challenges",
	    "networkMode": "awsvpc",
   	    "runtimePlatform": {
            "operatingSystemFamily": "LINUX"
   },
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 8000,
		    "protocol": "tcp"
                }
            ],
      "requiresCompatibilities": [
       "FARGATE"
    ]
        }
    ]
}
